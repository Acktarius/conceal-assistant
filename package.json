{
  "name": "conceal-assistant",
  "version": "1.2.0",
  "description": "Assistant pour Conceal",
  "main": "server.js",
  "nodemonConfig": {
    "watch": [
      "**/data/**"
    ],
    "ignore": [
      "**/data/infoSys.json",
      "**/data/infOSp.json",
      "**/public/**"
    ],
    "ext": "js,json,txt,pug,html",
    "delay": 1000
  },
  "scripts": {
    "start": "node server.js",
    "watch": "nodemon",
    "assistant": "nodemon server.js"
  },
  "keywords": [
    "Conceal",
    "CCX"
  ],
  "author": "Acktarius",
  "license": "BSD-3-Clause",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "conceal-api": "^0.8.8",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-flash": "0.0.2",
    "jsonwebtoken": "^9.0.0",
    "livereload": "^0.9.3",
    "local-ip-url": "^1.0.3",
    "method-override": "^3.0.0",
    "node-fetch": "^2.6.9",
    "pjson": "^1.0.9",
    "pug": "^3.0.2",
    "semver": "^7.5.3",
    "shelljs": "^0.8.5",
    "sysctlx": "^0.0.7",
    "winsc": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acktarius/conceal-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/Acktarius/conceal-assistant/issues"
  },
  "homepage": "https://github.com/Acktarius/conceal-assistant#readme"
}
