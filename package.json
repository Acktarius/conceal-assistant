    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-flash": "0.0.2",
    "jsonwebtoken": "^8.5.1",
    "livereload": "^0.9.3",
    "local-ip-url": "^1.0.3",
    "method-override": "^3.0.0",
    "pug": "^3.0.2",
    "sysctlx": "^0.0.7"
  },
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "nodemon": "^2.0.20"
  },
  "repository": {
    "type": "git",
