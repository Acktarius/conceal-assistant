{
  "name": "conceal-assistant",
  "version": "1.0.2",
  "description": "Assistant pour Conceal",
  "main": "server.js",
  "nodemonConfig": {
    "watch": [
      "**/data/**"
    ],
    "ignore": [
      "**/public/**"
    ],
    "ext": "js,json,txt",
    "delay": 500
  },
  "scripts": {
    "start": "node server.js",
